		config RTK_VOIP_COPROCESS_DSP_IS_HOST
			depends on ( RTK_VOIP_COPROCESS_DSP && RTK_VOIP_IPC_ARCH_IS_HOST )
			bool
			default y
		
		config RTK_VOIP_COPROCESS_DSP_IS_DSP
